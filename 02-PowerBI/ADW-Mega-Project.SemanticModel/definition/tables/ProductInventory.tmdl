	partition ProductInventory = calculated
		mode: import
		source = ```
				
				ADDCOLUMNS(
				    SUMMARIZE(
				        DimProduct,
				        DimProduct[ProductID],
				        DimProduct[ProductName],
				        DimProduct[CategoryName],
				        DimProduct[SubcategoryName]
				    ),
				    "StockQty", CALCULATE(SUM(FactInventory[Quantity])),
				    "InventoryValue", CALCULATE(SUM(FactInventory[InventoryValue])),
				
				    -- Stock Status Column
				    "StockStatus",
				        VAR Stock = CALCULATE(SUM(FactInventory[Quantity]))
				        RETURN
				            SWITCH(
				                TRUE(),
				                ISBLANK(Stock), "Out of Stock",
				                Stock = 0, "Out of Stock",
				                Stock < 50, "Critical",
				                Stock < 100, "Low",
				                Stock < 500, "Normal",
				                "Overstocked"
				            )
				)
				
				```

	annotation PBI_Id = e516e0147f1847378b2f3fd9028970ca

