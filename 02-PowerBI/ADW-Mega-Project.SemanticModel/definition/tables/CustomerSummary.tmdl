	partition CustomerSummary = calculated
		mode: import
		source = ```
				
				ADDCOLUMNS(
				    SUMMARIZE(DimCustomer, DimCustomer[CustomerID], DimCustomer[CustomerName]),
				    "TotalRevenue", CALCULATE([Total Revenue]),
				    "TotalOrders", CALCULATE([Total Orders]),
				    "CLV", DIVIDE(CALCULATE([Total Revenue]), CALCULATE([Total Orders]))
